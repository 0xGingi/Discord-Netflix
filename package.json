{
  "name": "Netflix",
  "version": "1.0.5",
  "description": "A Netflix wrapper with Discord Rich Presence",
  "main": "src/index.js",
  "scripts": {
    "start": "electron . ",
    "macbuild": "electron-builder -m",
    "linbuild": "electron-builder -l",
    "winbuild": "electron-builder -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Terroriser1/Discord-Netflix.git"
  },
  "author": "Terroriser1",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Terroriser1/Discord-Netflix/issues"
  },
  "build": {
    "appId": "com.netflix.Terroriser1",
    "win": {
      "target": "nsis-web",
      "icon": "./assets/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "./assets/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "./assets/icon.png"
    },
    "artifactName": "${productName}_Setup_${version}.${ext}",
    "electronVersion": "13.1.8",
    "electronDownload": {
      "version": "13.1.8-wvvmp",
      "mirror": "https://github.com/castlabs/electron-releases/releases/download/v"
    }
  },
  "homepage": "https://github.com/Terroriser1/Discord-Netflix#readme",
  "dependencies": {
    "discord-rpc": "4.0.1",
    "electron-discord-register": "1.0.0",
    "glob-to-regexp": "0.4.1",
    "jquery": "3.6.0",
    "moment": "2.29.1",
    "setimmediate": "1.0.5",
    "socket.io-client": "4.2.0",
    "update-electron-app": "^2.0.1",
    "xmldom": "github:xmldom/xmldom#0.7.0"
  },
  "devDependencies": {
    "electron": "13.3.0",
    "electron-builder": "^22.9.1",
    "electron-notarize": "1.0.1",
    "electron-reload": "1.5.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "register-scheme": "1.0.0"
  }
}
