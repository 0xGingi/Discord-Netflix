{
  "name": "Netflix",
  "version": "1.0.1",
  "description": "A Netflix wrapper with Discord Rich Presence",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "macbuild": "electron-builder -m",
    "linbuild": "electron-builder -l",
    "winbuild": "electron-builder -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nirewen/discord-netflix.git"
  },
  "keywords": [
    "discord",
    "rich",
    "presence",
    "netflix"
  ],
  "author": "Terroriser1",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nirewen/discord-netflix/issues"
  },
  "build": {
    "electronVersion": "13.0.0",
    "electronDownload": {
      "version": "13.0.0-wvvmp",
      "mirror": "https://github.com/castlabs/electron-releases/releases/download/v"
    },
    "appId": "com.netflix.terroriser1",
    "win": {
      "target": "nsis-web",
      "icon": "./assets/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "./assets/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "./assets/icon.png"
    }
  },
  "homepage": "https://github.com/nirewen/discord-netflix#readme",
  "dependencies": {
    "discord-rpc": "4.0.1",
    "electron-discord-register": "1.0.0",
    "jquery": "3.6.0",
    "moment": "2.29.1",
    "setimmediate": "1.0.5",
    "socket.io-client": "4.1.3"
  },
  "devDependencies": {
    "electron": "github:castlabs/electron-releases#v13.0.0-wvvmp",
    "electron-builder": "22.11.9",
    "electron-notarize": "1.0.0",
    "electron-reload": "1.5.0",
    "eslint": "7.31.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "register-scheme": "0.0.2"
  }
}
